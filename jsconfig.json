{
    "compilerOptions": {
        "target": "esnext",
        "baseUrl": "./",
        "jsx": "react",
        "paths": {
            "@app/*": ["src/*"],
            "@store/*": ["src/store/*"],
            "@store": ["src/store/"],
            "@components": ["src/components/index"],
            "@containers/*": ["src/containers/*"],
            "@pages/*": ["src/pages/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*", "build"]
}
